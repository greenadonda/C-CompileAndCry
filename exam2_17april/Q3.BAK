         case 'f':
         case 'F':
            printf("your grade is %c.",grade);
            switch(grade)
            {
               case 'f':
               case 'F': 
                printf("sorry you failed\n");
            }
        break;
        default:
            printf("invalid marks");
     }
     
      if(grade=='A' || grade=='B' || grade=='C' || grade=='D')
            {
                printf("you are elegible for the next level\n");
            }
            else
            {
                printf("plese try again next time");

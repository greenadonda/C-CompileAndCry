#include<stdio.h>
#include<conio.h>
 
main()
 {
     int marks;
     char grade;
     clrscr();
     
    printf("please enter your marks\n");
    scanf("%d",&marks);
     
     
         (marks >=90 && marks<=100)?grade='A': 	
	     (marks >=75 && marks<=89)?grade='B':
	     (marks>=60 && marks<=74)?grade='C':
        (marks>=50 && marks<=59)?grade='D':
	    (marks>=35 && marks<=49)?grade='E':
		(marks >=0 && marks<=34)?grade='F':'I';
		

     
     switch(grade)
     {
         case 'a':
         case 'A':
            printf("your grade is %c",grade);
            switch(grade)
            {
                case 'a':
                case 'A':
                    printf(".Excellent work!\n");
            }
        break;
            
        case 'b':
         case 'B':
            printf("your grade is %c.",grade);
            switch(grade)
            {
                case 'b':
                case 'B':
                    printf("Well done!\n");
            }
        break;
        
         case 'c':
         case 'C':
            printf("your grade is %c.",grade);
            switch(grade)
            {
                case 'c':
                case 'C':  
                    printf("Good job!\n");
            }
        break;
        
         case 'd':
         case 'D':
            printf("your grade is %c.",grade);
            switch(grade)
            {
                case 'd':
                case 'D':
                    printf("You passed, but you could do better\n");
            }
            
        break;
        
         case 'e':
         case 'E':
            printf("your grade is %c.",grade);
